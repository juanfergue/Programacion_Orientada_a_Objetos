	public Sociedad() {
		membersInActivities = new HashMap<>();
	}
	public void leerDeFichero(String fich) {
		try(Scanner sc = new Scanner(new File(fich))){
			
		}catch(Exception e) {
			throw new SociedadException("ERRO al leer el fichero: " + fich);
		}
	}

}
